Subject: [PATCH 03/24] build: Don't install lib.a.desc file since it breaks
 make install

---
 js/src/config/rules.mk | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/js/src/config/rules.mk b/js/src/config/rules.mk
index 42728d3..78ad302 100644
--- a/js/src/config/rules.mk
+++ b/js/src/config/rules.mk
@@ -137,7 +137,7 @@ REAL_LIBRARY		:= $(LIB_PREFIX)$(STATIC_LIBRARY_NAME).$(LIB_SUFFIX)
 ifeq (,$(SDK_LIBRARY)$(DIST_INSTALL)$(NO_EXPAND_LIBS))
 LIBRARY			:= $(REAL_LIBRARY).$(LIBS_DESC_SUFFIX)
 else
-LIBRARY			:= $(REAL_LIBRARY) $(REAL_LIBRARY).$(LIBS_DESC_SUFFIX)
+LIBRARY			:= $(REAL_LIBRARY)
 endif
 endif # STATIC_LIBRARY_NAME
 endif # LIBRARY
